{
  "builds": [
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    
    {
      "src": "^/admin/(?:login.html|dashboard.html|index.html|css/.*|js/.*)$",
      "dest": "/admin/$1"
    },
    {
      "src": "^/admin(?:/.*)?$",
      "dest": "/admin/login.html"
    },
    
   
    {
      "src": "/inquiry",
      "dest": "/server/server.js"
    },
    {
      "src": "/admin/login",
      "dest": "/server/server.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/server/server.js"
    },
    
   
    {
      "src": "^/(.*)",
      "dest": "/public/$1"
    }
  ]
}